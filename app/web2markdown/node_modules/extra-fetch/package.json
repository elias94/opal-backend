{
  "name": "extra-fetch",
  "version": "1.3.0",
  "description": "",
  "keywords": [],
  "files": [
    "lib",
    "dist"
  ],
  "main": "lib/es2018/index.js",
  "types": "lib/es2018/index.d.ts",
  "browser": {
    "./lib/es2015/utils/fetch.js": "./lib/es2015/utils/fetch.browser.js",
    "./lib/es2018/utils/fetch.js": "./lib/es2018/utils/fetch.browser.js",
    "./lib/es2015/utils/headers.js": "./lib/es2015/utils/headers.browser.js",
    "./lib/es2018/utils/headers.js": "./lib/es2018/utils/headers.browser.js",
    "./lib/es2015/utils/request.js": "./lib/es2015/utils/request.browser.js",
    "./lib/es2018/utils/request.js": "./lib/es2018/utils/request.browser.js",
    "./lib/es2015/utils/response.js": "./lib/es2015/utils/response.browser.js",
    "./lib/es2018/utils/response.js": "./lib/es2018/utils/response.browser.js",
    "./lib/es2015/utils/form-data.js": "./lib/es2015/utils/form-data.browser.js",
    "./lib/es2018/utils/form-data.js": "./lib/es2018/utils/form-data.browser.js",
    "./lib/es2015/utils/abort-controller.js": "./lib/es2015/utils/abort-controller.browser.js",
    "./lib/es2018/utils/abort-controller.js": "./lib/es2018/utils/abort-controller.browser.js",
    "./lib/es2015/utils/blob.js": "./lib/es2015/utils/blob.browser.js",
    "./lib/es2018/utils/blob.js": "./lib/es2018/utils/blob.browser.js",
    "./lib/es2015/utils/event-source.js": "./lib/es2015/utils/event-source.browser.js",
    "./lib/es2018/utils/event-source.js": "./lib/es2018/utils/event-source.browser.js"
  },
  "repository": "git@github.com:BlackGlory/extra-fetch.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src",
    "test": "jest --no-cache --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage --config jest.config.js",
    "prepublishOnly": "run-s clean build bundle",
    "clean": "run-p clean:*",
    "clean:build": "rimraf lib",
    "clean:bundle": "rimraf dist",
    "build": "run-p build:*",
    "build:es2015": "run-s build:es2015:*",
    "build:es2015:compile": "tsc --project tsconfig.build.json --module commonjs --target es2015 --outDir lib/es2015",
    "build:es2015:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib/es2015",
    "build:es2018": "run-s build:es2018:*",
    "build:es2018:compile": "tsc --project tsconfig.build.json --module commonjs --target es2018 --outDir lib/es2018",
    "build:es2018:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib/es2018",
    "bundle": "rollup --config rollup.config.js",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/jest-matchers": "^0.1.10",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/eventsource": "^1.1.5",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.18.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "msw": "^0.27.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.1.0",
    "ts-jest": "^26.5.3",
    "tscpaths": "^0.0.9",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "domexception": "^2.0.1",
    "eventsource": "https://github.com/EventSource/eventsource#46fe04e03e54f4129a28bf75b3a1e5f4ab68b52a",
    "fetch-blob": "^2.1.1",
    "formdata-node": "^2.4.0",
    "node-fetch": "3.0.0-beta.9"
  }
}
